import java.util.Scanner;
class Sample
{
    public static void main(String args[])
    {
        int n,k,c=0;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        k=sc.nextInt();
        int num[]=new int[n];
        for(int i=0;i<n;i++)
        {
            num[i]=sc.nextInt();
        }
        for(int i=0;i<n;i++)
        {
            if(num[i]%k!=0)
            c++;
        }
        System.out.println(c);
    }
}
